<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.insarouen.beaconSandwich" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="LocationManager">
        <param name="android-package" value="com.unarin.cordova.beacon.LocationManager" />
    </feature>
    <name>BeaconSandwich</name>
    <description>
        Application de suivi de flux via Beacon.
    </description>
    <author>
        BeaconSandwich Team
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
